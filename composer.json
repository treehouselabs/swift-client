{
  "name": "treehouselabs/swift-client",
  "type": "library",
  "description": "Client for the Swift object store protocol",
  "keywords": ["swift", "object store", "openstack"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    }
  ],
  "require": {
    "php": ">=5.6",
    "psr/log": "~1.0",
    "guzzlehttp/guzzle": "~6.0",
    "symfony/http-foundation": "~2.1",
    "treehouselabs/keystone-client": "~3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\Swift\\": "src/TreeHouse/Swift"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\Swift\\Tests\\": "tests/TreeHouse/Swift/Tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
